    script(src='https://unpkg.com/sweetalert/dist/sweetalert.min.js')
    script.
        var socket = io.connect(window.location.hostname) ; 
      $(document).ready(function(){
          socket.emit('online','anonymous') ; 
          
          var password = document.getElementById("pass") ;
          var confirm_password = document.getElementById("repass");
          function validatePassword(){
                if(password.value != confirm_password.value) {
                confirm_password.setCustomValidity("Passwords Don't Match");
                } else {
                confirm_password.setCustomValidity('');
          }
          }
          password.onchange = validatePassword;
          confirm_password.onkeyup = validatePassword;


          $('#upperForm').submit(function(){
            socket.emit('profile',{id : 'admin' , pass : $('#upperPass').val()}) ; 
          })
          
          $('#downForm').submit(function(){
            socket.emit('profile',{id : 'admin' , pass : $('#downPass').val()}) ; 
          })
          socket.on('changeSuccess',function(){
            swal(
                {
                  title : "Thông báo", 
                  text : "Đổi thành công", 
                  icon : "success",
                  button : false 
                }
              );
          })
          socket.on('wrongPass',function(){ 
              swal(
                {
                  title : "Thông báo", 
                  text : "Sai mật khẩu", 
                  icon : "error",
                  button : false 
                }
              );
          })
      
      });